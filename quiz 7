# Question 1

count = 0
def my_seq(n):
    global count
    count += 1
    if n == 0:
        return 1
    if n == 1:
        return -5
    else:
        return 3*my_seq(n - 1) + 28*my_seq(n - 2)
print my_seq(1)

# Question 2

def super(n):
    if n == 1:
        return 7
    return n*super(n-1)

def is_super(n):
    if n > 1:
        return True
    else:
        return False
print(super(1))
